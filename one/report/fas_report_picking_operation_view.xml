<odoo>
  <template id="fas_report_picking_operation" inherit_id="stock.report_picking">]
      <xpath expr="//img" position="replace">
      </xpath> 
      <xpath expr="//h1" position="replace">
      <h1 align="center">
        <t>
          <span t-if="o.picking_type_id.code in ['outgoing']"><strong>WAREHOUSE PASS#</strong></span>
          <span t-if="o.picking_type_id.code in ['incoming']"><strong>RECEIVING REPORT#</strong></span>
          <span t-if="o.picking_type_id.code in ['internal']"><strong>INTERNAL TRANSFER#</strong></span>
         </t>
          <span t-field="o.name"/>
      </h1>    
      </xpath>
      <xpath expr="//tr/th[3]" position="replace">
          <th><strong>Order Date</strong></th>
      </xpath>
      <xpath expr="//tr/th[4]" position="replace">
          <th><strong>Released Date</strong></th>
      </xpath>
      <xpath expr="//table[3]" position="after">
         <span><strong>&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;Total Quantity: <span t-field="o.total_product_qty"/></strong></span><br/>
      </xpath>
      <xpath expr="//div" position="after">
        <div class="footer">
            <table width="100%">
                <tr>
                    <td align="center"><strong>Prepared By:</strong></td>
                    <td align="center"><strong>Checked By:</strong></td>                  
                    <td align="center"><strong>Approved By:</strong></td>
                    </tr>
                    <tr>
                    <td><br/><br/></td>
                    <td><br/><br/></td>
                    <td><br/><br/></td>
                    <td><br/><br/></td>
                    </tr>
                    <tr>
                    <td align="center">__________________</td>
                    <td align="center">__________________</td>
                    <td align="center">__________________</td>
                    </tr>

            </table>

        </div>
        </xpath>
  </template>
</odoo>