<?xml version="1.0"?>
<odoo>
    <data>
        <record id="fos_view_account_journal_form" model="ir.ui.view">
            <field name="name">fos.view.account.journal.form</field>
            <field name="model">account.move</field>
            <field name="inherit_id" ref="account.view_move_form"/>
            <field name="arch" type="xml">
                <xpath expr="//field[@name='date']" position="attributes">
                  <attribute name="attrs">{'readonly': ['1','=','0']}</attribute>
                </xpath>
            </field>
        </record>
    </data>
</odoo>