<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record model="ir.ui.view" id="fos_service_technician_tree">
            <field name="name">Service Technicians</field>
            <field name="model">fos.service.technician</field>
            <field name="arch" type="xml">
                <tree string="Service Technicians" delete="false">
                    <field name="name"/>
                    <field name="active"/>
                </tree>
            </field>
        </record>
        
        <record model="ir.ui.view" id="fos_service_technician_form">
            <field name="name">Service Technicians</field>
            <field name="model">fos.service.technician</field>
            <field name="arch" type="xml">
                <form string="Service Technician" delete="false">
                    <sheet>
                        <group>
                            <field name="name"/>
                            <field name="active"/>
                        </group>
                    </sheet>
                </form>
            </field>
        </record>
        
        <record id="fos_service_technician_act_window" model="ir.actions.act_window">
            <field name="name">Service Technician</field>
            <field name="type">ir.actions.act_window</field>
            <field name="res_model">fos.service.technician</field>
            <field name="view_type">form</field>
            <field name="view_mode">tree,form</field>
        </record>

        <menuitem id="fos_service_technician_menu" 
            name="Service Technicians" 
            action="fos_service_technician_act_window" 
            parent="one.fos_service_maintenance_menu" 
            sequence="10"
        />
    </data>
</odoo>