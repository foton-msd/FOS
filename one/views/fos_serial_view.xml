<odoo>
  <data>
    <record id="fos_view_serial_view_form" model="ir.ui.view">
      <field name="name">fos.serial.form</field>
      <field name="model">stock.production.lot</field>
      <field name="inherit_id" ref="stock.view_production_lot_form"/>
      <field name="arch" type="xml">
        <xpath expr="//group/group/field[2]" position="after">
          <field name="fos_fu_id" options="{'no_open': True, 'no_create': True, 'no_quick_create': True, 'no_create_edit': True}"/> 
        </xpath>
      </field>
    </record>
  </data>
</odoo>    